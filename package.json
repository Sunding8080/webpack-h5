{
  "name": "webpack-h5",
  "version": "1.0.0",
  "description": "webpack h5 base template",
  "repository": "git@github.com:Sunding8080/webpack-h5.git",
  "author": "Sunding8080 <1141544903@qq.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env RUN_ENV=local webpack serve --config=./build/webpack.config.dev.js",
    "dev:proxy": "cross-env RUN_ENV=local RUN_PROXY=true webpack serve --config=./build/webpack.config.dev.js",
    "dev:eslint": "cross-env RUN_ENV=local RUN_ESLINT=true webpack serve --config=./build/webpack.config.dev.js",
    "dev:proxy:eslint": "cross-env RUN_ENV=local RUN_ESLINT=true RUN_PROXY=true webpack serve --config=./build/webpack.config.dev.js",
    "build:dev": "cross-env RUN_ENV=development webpack build --config=./build/webpack.config.prod.js",
    "build": "cross-env RUN_ENV=production webpack build --config=./build/webpack.config.prod.js",
    "analyse": "cross-env RUN_ENV=production webpack build --config=./build/webpack.config.prod.analyse.js",
    "eslint": "eslint ./src/**/*.js",
    "lint": "eslint ./src/**/*.js --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime-corejs3": "^7.15.3",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.2",
    "internal-ip": "^6.2.0",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^2.2.0",
    "mocker-api": "^2.9.0",
    "postcss-loader": "^6.1.1",
    "postcss-nested": "^5.0.6",
    "precss": "^4.0.0",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "stylus-loader": "^6.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.47.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "husky": "4",
    "lint-staged": "10.2.9",
    "prettier": "^2.3.2",
    "webpack-dev-server": "^4.0.0-rc.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add ."
    ],
    "src/**/*.{css,.less,.scss,.sass,.json}": [
      "prettier --write",
      "git add ."
    ]
  }
}
